<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        tools:targetApi="31"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:dist="http://schemas.android.com/apk/distribution">

    <dist:module dist:instant="true"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.PlanAI_front"
            android:name=".create.MainCalendarApplication"
            android:networkSecurityConfig="@xml/network_security_config">
        <activity
            android:name=".WriteActivity"
            ></activity>
        <activity
            android:name=".MywriteActivity"
             ></activity>
        <activity
            android:name=".GetfriendActivity"
             ></activity>
        <activity
            android:name=".FriendlistActivity"
             ></activity>
        <activity
            android:name=".AddfriendActivity"
             ></activity>
        <activity
            android:name=".BoardActivity"
             ></activity>
        <activity android:name=".Profile.SettingActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.planai_front.GoogleSignInActivity"></activity>
        <activity android:name=".MaterialCalendarActivity"></activity>
        <activity android:name=".create.ShowDayActivity"></activity>
        <activity android:name=".create.CreateScheduleActivity"
                  android:theme="@style/PopupActivity">
        </activity>
        <activity android:name=".create.CreateTaskActivity"
                  android:theme="@style/PopupActivity">
        </activity>

    </application>
    <!--서버-->
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>

